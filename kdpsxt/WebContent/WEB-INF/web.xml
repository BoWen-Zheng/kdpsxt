   </filter>
  
   <filter-mapping>
  	<filter-name>CrossFilter</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
    
  <servlet-mapping>
    <servlet-name>default</servlet-name>
    <url-pattern>*.jpg</url-pattern>
</servlet-mapping>
  <servlet-mapping>
